{
  "name": "@penxle/penxle",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^18 || ^20",
    "pnpm": "^8.6.0"
  },
  "scripts": {
    "bootstrap": "pnpm install && husky install",
    "dev": "doppler run --command 'vite dev'",
    "build": "vite build",
    "codegen": "pnpm run codegen:kit && pnpm run codegen:db && pnpm run codegen:gql",
    "codegen:kit": "doppler run --command 'pnpm exec svelte-kit sync'",
    "codegen:db": "prisma generate",
    "codegen:gql": "houdini generate",
    "db:migrate": "doppler run --command 'pnpm exec prisma migrate dev'",
    "db:migrate:create": "doppler run --command 'pnpm exec prisma migrate dev --create-only'",
    "lint": "eslint --max-warnings=0 .",
    "format": "eslint --fix .",
    "check": "svelte-check --fail-on-warnings",
    "test": "playwright test",
    "vercel-build": "svelte-kit sync && prisma generate && pnpm run build && prisma migrate deploy"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.370.0",
    "@aws-sdk/s3-request-presigner": "^3.370.0",
    "@envelop/core": "^4.0.0",
    "@envelop/graphql-jit": "^6.0.1",
    "@envelop/types": "^4.0.0",
    "@felte/core": "^1.4.0",
    "@felte/validator-zod": "^1.0.16",
    "@floating-ui/dom": "^1.4.5",
    "@iconify-json/lucide": "^1.1.110",
    "@iconify-json/simple-icons": "^1.1.60",
    "@iconify/utils": "^2.1.7",
    "@logtail/pino": "^0.4.4",
    "@playwright/test": "^1.36.1",
    "@pothos/core": "^3.30.0",
    "@pothos/plugin-prisma": "^3.52.0",
    "@pothos/plugin-scope-auth": "^3.19.2",
    "@pothos/plugin-simple-objects": "^3.7.0",
    "@pothos/plugin-validation": "^3.9.2",
    "@prisma/client": "^5.0.0",
    "@sentry/sveltekit": "^7.58.1",
    "@svelte-kits/store": "^0.1.2",
    "@sveltejs/adapter-vercel": "^3.0.2",
    "@sveltejs/kit": "^1.22.3",
    "@tiptap/core": "^2.0.3",
    "@tiptap/extension-bold": "^2.0.3",
    "@tiptap/extension-document": "^2.0.3",
    "@tiptap/extension-hard-break": "^2.0.3",
